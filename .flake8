[flake8]
ignore = D205, D400, I101, ANN101, ANN002, ANN003, W503
max-line-length = 88
exclude =
  .git
  venv
  __pycache__
  source
  outputs
  docs/*
  analysis/*
  codebuild/*
  disease/version.py
inline-quotes = "
import-order-style = pep8
application-import-names =
  disease
  tests
per-file-ignores =
  tests/*:ANN001, ANN2
  *__init__.py:F401
mypy-init-return = true
